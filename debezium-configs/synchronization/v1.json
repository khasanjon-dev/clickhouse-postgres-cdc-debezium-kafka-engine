{
  "name": "all-tables-connector",
  "config": {
    "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
    "database.hostname": "172.17.0.1",
    "database.port": "5432",
    "database.user": "postgres",
    "database.password": "1",
    "database.dbname": "postgres",
    "database.server.name": "pg",
    "plugin.name": "pgoutput",
    "publication.name": "all_tables",
    "slot.name": "all_tables",
    "topic.prefix": "all",
    "topic.creation.enable": "true",
    "tasks.max": "20",
    "topic.creation.default.cleanup.policy": "delete",
    "topic.creation.default.partitions": "3",
    "topic.creation.default.replication.factor": "1",
    "topic.creation.default.retention.ms": "604800000",
    "table.include.regex": ".*",
    "table.exclude.list": ".*\\.ddl_logs",
    "snapshot.mode": "initial",
    "tombstones.on.delete": "false",
    "transforms": "changes,route",
    "transforms.changes.type": "io.debezium.transforms.ExtractChangedRecordState",
    "transforms.route.type": "org.apache.kafka.connect.transforms.RegexRouter",
    "transforms.route.regex": "all\\.(.*)",
    "transforms.route.replacement": "all_tables",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "decimal.handling.mode": "double",
    "time.precision.mode": "connect"
  }
}